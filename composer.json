{
    "name": "qlimix/uuid-ramsey",
    "type": "library",
    "description": "Ramsey uuid generator implementation",
    "license": "MIT",
    "keywords": [
        "uuid",
        "ramsey"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">7.4",
        "qlimix/uuid": "^2.0",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "qlimix/code-standard": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\Id\\Uuid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\Id\\Uuid\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
